      "inputs": { "required": ["material_id"] },
      "outputs": { "primary": "taylor_coefficients", "secondary": ["n", "C"] },
      "skills_used": ["prism-material-physics"],
      "latency_target_ms": 30
    },
    {
      "id": "CAP-MLOOK-006",
      "name": "ISO Group Lookup",

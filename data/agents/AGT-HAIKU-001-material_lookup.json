      "outputs": { "primary": "material_data" },
      "skills_used": ["prism-material-lookup"],
      "latency_target_ms": 100
    },
    {
      "id": "CAP-MLOOK-003",
      "name": "Property Retrieval",
      "description": "Retrieves specific property for a material",
      "category": "LOOKUP",
      "inputs": { "required": ["material_id", "property_name"] },
      "outputs": { "primary": "property_value" },
      "skills_used": ["prism-material-lookup", "prism-material-schema"],
      "latency_target_ms": 30
    },
    {
      "id": "CAP-MLOOK-004",

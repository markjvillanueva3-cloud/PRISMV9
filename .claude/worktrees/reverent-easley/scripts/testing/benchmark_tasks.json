  "scoringRules": {
    "domainMatch": {
      "weight": 0.3,
      "calculation": "jaccard_similarity(actual_domains, expected_domains)"
    },
    "operationMatch": {
      "weight": 0.25,
      "calculation": "coverage(actual_operations, expected_operations)"
    },
    "skillMatch": {
      "weight": 0.2,
      "calculation": "overlap_count(actual_skills, expected_skills) / len(expected_skills)"
    },
    "agentMatch": {
      "weight": 0.15,
      "calculation": "overlap_count(actual_agents, expected_agents) / len(expected_agents)"
    },
    "psiScore": {
      "weight": 0.1,
      "calculation": "actual_psi >= min_psi_score ? 1.0 : actual_psi / min_psi_score"
    }
  }
